<?xml version='1.0'?>
<!DOCTYPE hibernate-mapping PUBLIC>

<hibernate-mapping package="net.common.model">

<class name="Person" table="person">
	<id name="id" type="int" column="id" >
		<generator class="assigned"/>
	</id>

	<property name="name">
		<column name="Name" />
	</property>
</class>

</hibernate-mapping>
